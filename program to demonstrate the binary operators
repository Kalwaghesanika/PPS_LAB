#include <iostream>
using namespace std;
int main() 
{
  int a=10,b=5;
  //Addition operator 
  cout<<"a+b="<<(a+b)<<endl;
  
  //Subtraction operator 
  cout<<"a-b="<<(a-b)<<endl;
  
  //Multiplication opertor 
  cout<<"a*b="<<(a*b)<<endl;
  
  //Division operator 
  cout<<"a/b="<<(a/b)<<endl;
  
  //Modulo operator
  cout<<"a%b="<<(a%b)<<endl;
  return 0;
}